@model IEnumerable<WebApplication2.CATEGORIE>

@{
    ViewBag.Title = "Index";
}

<h2>Fournisseur</h2>
@{
    Layout = "~/Views/Shared/_Admin.cshtml";
}
<div class="container">
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Libelle)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Libelle)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.CategorieID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.CategorieID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.CategorieID })
                </td>
            </tr>
        }

    </table>
</div>